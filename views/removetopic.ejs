<%- include("partials/header.ejs") %> 

<div class="main">
    <div class="topic-title">
        <h2>TOPIC NAMES</h2>
    </div>
    <div class="rem-topic-mid">
        <% t.forEach(function(item){ %>

            <div class="rem-topiclist">
                <div style="background-color: #3AAFA9;"><%= _.capitalize(item.topicname) %></div>
                <form action="/remove" method="post" class="rem-top">
                    <button class="btn" style="width: 50px;" name="rembtn" value="<%= item.topicname %>"">-</button>
                </form>
            </div>

        <% }); %>
    </div>
</div>


<%- include("partials/footer.ejs") %>